<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="portal_layout" name="App Deeplink" inherit_id="portal.portal_my_home">
        <xpath expr="//div[@id='oe_structure_portal_my_home_2']" position="after">
            <h3 style="margin-top: 12px;">App List</h3>
            <div class="portal_apps">
                <div class="background-order">
                    <div class="first-background-col">
                        <div class="categories_title">
                            <i class="fa fa-folder o_search_panel_section_icon mr-2"/>
                            <b>CATEGORIES</b>
                        </div>
                        <ul class="button-all-item">
                            <li>All</li>
                        </ul>
                    </div>
                    <div class="last-background-col"></div>
                </div>
            </div>
        </xpath>
    </template>
</odoo>
