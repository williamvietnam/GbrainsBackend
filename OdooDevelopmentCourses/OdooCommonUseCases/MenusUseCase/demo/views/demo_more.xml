<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--Sub Demo 1-->
    <record id="sub_demo1_view_tree" model="ir.ui.view">
        <field name="name">sub.demo1.view.tree</field>
        <field name="model">sub.demo1</field>
        <field name="arch" type="xml">
            <tree export_xlsx="0">
                <field name="name" optional="show"/>
                <field name="detail" optional="show"/>
            </tree>
        </field>
    </record>

    <record id="sub_demo1_view_form" model="ir.ui.view">
        <field name="name">sub.demo1.view.form</field>
        <field name="model">sub.demo1</field>
        <field name="arch" type="xml">
            <form duplicate="false">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                    <group>
                        <field name="detail"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!--Sub Demo 2-->
    <record id="sub_demo2_view_tree" model="ir.ui.view">
        <field name="name">sub.demo2.view.tree</field>
        <field name="model">sub.demo2</field>
        <field name="arch" type="xml">
            <tree export_xlsx="0">
                <field name="name" optional="show"/>
                <field name="detail" optional="show"/>
            </tree>
        </field>
    </record>

    <record id="sub_demo2_view_form" model="ir.ui.view">
        <field name="name">sub.demo2.view.form</field>
        <field name="model">sub.demo2</field>
        <field name="arch" type="xml">
            <form duplicate="false">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                    <group>
                        <field name="detail"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
